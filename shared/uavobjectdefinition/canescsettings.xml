<xml>
    <object name="CanEscSettings" singleinstance="true" settings="false">
        <description>CAN-ESC Settings</description>
        <field name="Flags" units="Flag" type="enum" options="FALSE,TRUE" elementnames="DebugPrintEn,BrakeEn,LoadDefaults" />

        <!-- 2014-04-21 12:08:57 -> 14, 4, 21, 12, 8, 57  -->
        <field name="BuildDate" units="" type="uint8" elements="6" defaultvalue="0"/>
        <!-- #3 GIT.abff8c8  -> 3, abff8c8 -->
        <field name="BuildVersion" units="" type="uint32" elements="2" defaultvalue="0"/>
        <!-- BLDC address -->
        <field name="ESCAddr" units="" type="uint8" elements="1" defaultvalue="0"/>
        <!-- Serial no of cortex CPU (96 Bit) -->
        <field name="HwSerialNo" units="" type="uint32" elements="3" defaultvalue="0"/>

        <field name="MotorPolePairs" units="" type="uint8" elements="1" defaultvalue="0"/>
        <field name="MotorKv" units="RPM/V" type="uint16" elements="1" defaultvalue="0"/>
        <field name="MotorMaxCurrent" units="A" type="float" elements="1" defaultvalue="0"/>
        <field name="MotorMaxPower" units="W" type="uint16" elements="1" defaultvalue="0"/>
 
        <field name="MaxAcceleration" units="Ticks/ms^2" type="uint16" elements="1" defaultvalue="0"/>
        <field name="MaxCycleTime" units="ms" type="uint16" elements="1" defaultvalue="0"/>

        <field name="AlignTime" units="ms" type="uint16" elements="1" defaultvalue="0"/>
        <field name="AlignCurrent" units="mA" type="uint16" elements="1" defaultvalue="0"/>

        <field name="RampUpTime" units="ms" type="uint16" elements="1" defaultvalue="0"/>
        <field name="RampUpStartPeriod" units="us" type="uint16" elements="1" defaultvalue="0"/>
        <field name="RampUpEndPeriod" units="us" type="uint16" elements="1" defaultvalue="0"/>

        <field name="MinPWM" units="%" type="float" elements="1" defaultvalue="0.0"/>
        <field name="MaxPWM" units="%" type="float" elements="1" defaultvalue="0.0"/>
        <field name="MinBatVoltage" units="V" type="float" elements="1" defaultvalue="0.0"/>

        <access gcs="readwrite" flight="readwrite"/>
        
        <telemetrygcs acked="false" updatemode="onchange" period="0"/>
        <telemetryflight acked="false" updatemode="onchange" period="0"/>
        <logging updatemode="manual" period="0"/>
    </object>
</xml>
